{
  "connection": "default",
  "collectionName": "authors",
  "info": {
    "name": "author"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "bio": {
      "type": "text"
    },
    "email": {
      "type": "email"
    },
    "posts": {
      "collection": "post"
    }
  }
}
